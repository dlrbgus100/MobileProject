<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/backgound"
    tools:context="kr.ac.kumoh.ce.s20100884.database.DetailPet">
<TextView
    android:id="@+id/detailPetType"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textColor="#8a8a8a" />
    <TextView
        android:id="@+id/detailPetSize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#8a8a8a" />
    <TextView
        android:id="@+id/detailPetPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#8a8a8a" />
    <TextView
        android:id="@+id/detailPetTerm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#8a8a8a" />
    <TextView
        android:id="@+id/detailPetAddress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#8a8a8a" />
    <TextView
        android:id="@+id/detailPetEmail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#8a8a8a" />
    <TextView
    android:id="@+id/detailPetNumber"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
        android:textColor="#8a8a8a" />
    <TextView
        android:id="@+id/detailPetMemo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#8a8a8a" />
</LinearLayout>
